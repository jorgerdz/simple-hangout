{
  "manifest_version": 2,

  "name": "Simple Hangout",
  "description": "This extension hides unnecesary hangouts.google.com assets to make it simpler visually",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://hangouts.google.com/*"],
      "js": ["hangout.js"]
    }
  ],
  "icons": 
    { 
      "16": "img/icon_16.png",
      "48": "img/icon_48.png",
      "128": "img/icon_128.png" 
    }
}